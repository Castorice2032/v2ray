{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProxyTypeSchema",
  "description": "Schema for describing detected proxy type and structure",
  "type": "object",
  "properties": {
    "category": {
      "description": "Top-level classification of the input",
      "type": "string",
      "enum": [
        "yaml",
        "json",
        "subscription",
        "single_proxy",
        "unknown",
        "error"
      ]
    },
    "type": {
      "description": "Specific type within the category",
      "type": "string",
      "enum": [
        "clash_yaml",
        "proxy_object_json",
        "subscription_base64",
        "subscription_base64_invalid",
        "subscription_plain",
        "subscription_plain_invalid",
        "vmess",
        "vless",
        "trojan",
        "ss",
        "reality",
        "hysteria",
        "unknown",
        "error"
      ]
    },
    "format": {
      "description": "Format of the data",
      "type": "string",
      "enum": [
        "object",
        "subscription_list",
        "single_proxy_link",
        "unknown"
      ]
    },
    "encoding": {
      "description": "Encoding of the input data",
      "type": "string",
      "enum": [
        "plain",
        "base64"
      ]
    },
    "protocols": {
      "description": "List of detected protocols in the input",
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "vmess",
          "vless",
          "trojan",
          "ss",
          "reality",
          "hysteria"
        ]
      }
    },
    "valid": {
      "description": "Whether the detected type is valid",
      "type": "boolean"
    },
    "raw_lines": {
      "description": "Number of non-empty lines in the input (for subscriptions)",
      "type": "integer"
    },
    "description": {
      "description": "Human-readable description of the detected type",
      "type": "string"
    },
    "url": {
      "description": "Source URL (if applicable)",
      "type": "string"
    },
    "error": {
      "description": "Error message (if type is error)",
      "type": "string"
    }
  },
  "additionalProperties": false
}
